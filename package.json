{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "env-cmd -f .env cross-env NODE_OPTIONS='--experimental-require-module' nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "env-cmd -f .env cross-env NODE_OPTIONS='--experimental-require-module' nest start",
    "start:dev": "env-cmd -f .env cross-env NODE_OPTIONS='--experimental-require-module' nest start --watch",
    "start:debug": "env-cmd -f .env cross-env NODE_OPTIONS='--experimental-require-module' nest start --debug --watch",
    "start:prod": "env-cmd -f .env cross-env NODE_OPTIONS='--experimental-require-module' node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "env-cmd -f .env cross-env NODE_OPTIONS='--experimental-require-module' node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate": "npx drizzle-kit generate",
    "migrate": "npx drizzle-kit migrate",
    "db:push": "npx drizzle-kit push"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "@influxdata/influxdb-client": "^1.35.0",
    "@kubernetes/client-node": "^0.22.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.7",
    "@nestjs/websockets": "^10.4.7",
    "@node-rs/argon2": "^2.0.0",
    "@types/helmet": "^4.0.0",
    "@types/newrelic": "^9.14.4",
    "@vercel/postgres": "^0.10.0",
    "auth0": "^4.10.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.34.1",
    "env-cmd": "^10.1.0",
    "express-jwt": "^8.4.1",
    "helmet": "^7.1.0",
    "jwks-rsa": "^3.1.0",
    "lucia": "^3.2.1",
    "newrelic": "^12.3.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "postgres": "^3.4.5",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "uuid": "^10.0.0",
    "xml2js": "^0.6.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/testing": "^10.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.25.0",
    "drizzle-zod": "^0.5.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
